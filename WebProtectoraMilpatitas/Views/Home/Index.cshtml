@model IEnumerable<ProtectoraMilpatitasGenNHibernate.EN.ProtectoraMilpatitas.AnimalEN>
@{
    ViewBag.Title = "Protectora Milpatitas";
}

<h3>Ultimos animales</h3>

<table>
    @foreach (var item in Model)
    {
    <tr>
        <td>
           @Html.DisplayFor(modelItem->item.Nombre);
        </td>
        <td>@Html.ActionLink("Ver Animal", "Animal", routeValues: new { id = item.Id }, htmlAttributes: null)</td>
    </tr>
    }
</table>